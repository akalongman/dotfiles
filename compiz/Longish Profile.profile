s0_put_bottomright_key = <Control><Alt>KP_Next

[addhelper]
s0_toggle_key = <Alt><Super>p
s0_ononinit = true
s0_brightness = 60
s0_saturation = 80

[rotate]
s0_rotate_to_1_key = <Super>Home
s0_rotate_to_1_window_key = <Shift><Super>Home
s0_rotate_to_2_window_key = <Alt>Tab

[vpswitch]
